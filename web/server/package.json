{
  "name": "tracelite-unified-server",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon --watch . index.js",
    "build": "cd .. && npm run build && cp -r dist/* server/public/",
    "build:win": "cd .. && npm run build && xcopy /E /I /Y dist server\\public",
    "serve": "npm run build:win && npm start"
  },
  "dependencies": {
    "better-sqlite3": "^8.4.0",
    "express": "^4.18.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
